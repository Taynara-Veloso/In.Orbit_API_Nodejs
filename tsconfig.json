{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.11.1",

  "compilerOptions": {
    "lib": ["es2022"],
    "module": "node16",
    "target": "es2022",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "forceConsistentCasingInFileNames": true /* Ensure that casing is correct in imports. */
  }
}
